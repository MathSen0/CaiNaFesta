@model SuporteModel

@if (TempData["Mensagem2"] != null)
{
    <div class="alert-success">
        @TempData["Mensagem2"]
    </div>
}
@if (TempData ["Mensagem3"] != null)
{
    <div class="alert-danger text-center">
        @TempData["Mensagem3"]
    </div>
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial scale=1,0" />
    <title>Suporte - Cai na Festa</title>
    <link rel="stylesheet" href="~/css/site.css" />
</head>

<body>
    @*    Logo do grupo*@
    <div style="display:inline-block">
@*        <img id="Logo" style="width:100px; height:100px" src="data:image/png;base64," />
*@        <h1 id="Suporte">Suporte</h1>
    </div>
    <div style="">
        <form asp-controller="Home" asp-action="Ticket" method="post">
            <label>Nome Completo:</label><br />
            <input type="text" asp-for="Nome" id="NomeSuporte" class="col-4" /><br />

            <label>Email:</label><br />
            <input type="email" asp-for="Email" id="Email" class="col-4" /><br />

            <label>Telefone:</label><br />
            <input type="text" asp-for="Telefone" id="Telefone" class="col-3" /><br />

            <label>No que podemos te ajudar?</label><br />
            <textarea type="text" asp-for="Descricao" id="Descricao" rows="6" cols="50" maxlength="300" wrap="soft"></textarea><br />
            <div class="text-left">
                <button type="submit" class="btn-primary">Enviar</button>
            </div>
        </form>
    </div>
    <p></p>

</body>
</html>